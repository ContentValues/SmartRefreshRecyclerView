<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qw.recyclerview.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".MyApplication"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QRecyclerView">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SmartRefreshLayoutActivity"/>
        <activity android:name=".SwipeRefreshLayout2Activity"/>
        <activity android:name=".SwipeRefreshLayout1Activity"/>
        <activity android:name=".SmartRefreshLayoutPlusActivity"/>
        <activity android:name=".SwipeRefreshLayoutPlusActivity"/>

    </application>

</manifest>